
comptime
  reset

  type MyType
    a Int
    b Int

  fn SomeContainer
    T1 Type
    T2 Type
    Type
    do
      var typeName > concat "SomeContainer" "<" T1 "," T2 ">"
      if > not >> typeExists typeName
        do
          type typeName
            a T1
            b T2
      typeName

  print > SomeContainer Int Int
  var a > new >> SomeContainer Int Int >> a: 123 >> b: 456

  print > itos a.b
  print > itos a.a